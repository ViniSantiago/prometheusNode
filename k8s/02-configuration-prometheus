## Criando o namespace
#kubectl create -f 10-namespace.yaml

## Criando a secret de acesso o registry
# kubectl create secret docker-registry registrysecretkeynhhb --namespace=box --docker-username=drachenfutter --docker-password=

## Criando volumes
kubectl create -f 17-persistent-volume-claim.yaml


## Criando o service
kubectl create -f 22-service-prometheus.yaml

## Criando o deployment
kubectl create -f 32-deployment-prometheus.yaml

## Criando o ingress
kubectl create -f 42-ingress-interno-prometheus.yaml
